{{ form_start(form) }}
<div>
	{{ form_errors(form) }}
</div>
<div>
	<div>
		{{ form_row(form.name) }}
	</div>
	<div>
		{{ form_row(form.description) }}
	</div>
	<div>
		{{ form_row(form.latitude) }}
	</div>
	<div>
		{{ form_row(form.longitude) }}
	</div>
	<div id="picture-form-container"></div>
	<div class="m-2">
		<button data-template='{{ form_widget(form.pictures.vars.prototype) }}' data-index='{{ form.pictures|length }}' type="button" id="add-picture-btn">Ajouter une photo</button>
	</div>
	<div class="m-2">
		<button class="btn btn-success">{{ button_label|default('Save') }}</button>
	</div>
	{{ form_end(form) }}
